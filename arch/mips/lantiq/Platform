#
# Lantiq
#

platform-$(CONFIG_LANTIQ)	+= lantiq/
cflags-$(CONFIG_LANTIQ)		+= -I$(srctree)/arch/mips/include/asm-generic/mach-lantiq
load-$(CONFIG_LANTIQ)		= 0xffffffff80002000
cflags-$(CONFIG_SOC_TYPE_XWAY)	+= -I$(srctree)/arch/mips/include/asm-generic/mach-lantiq/xway
cflags-$(CONFIG_SOC_FALCON)	+= -I$(srctree)/arch/mips/include/asm-generic/mach-lantiq/falcon
